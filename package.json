{
  "name": "eofol4",
  "version": "4.0.0",
  "description": "All-inclusive blazingly fast reactive web framework with zero configuration, batteries included!",
  "keywords": [],
  "scripts": {
    "start": "node ./scripts/start.js",
    "build": "node ./scripts/build.js",
    "serve": "node ./scripts/serve.js",
    "clean": "node ./scripts/clean.js",
    "test": "jest",
    "check": "npx eslint -c eslint.config.mjs .",
    "check-fix": "npx eslint -c eslint.config.mjs --fix .",
    "prettify": "npx prettier --config .prettierrc --write .",
    "lint": "npm run check-fix && npm run prettify",
    "analyze": "node ./scripts/analyze.js 1",
    "analyze-bundle": "npm run build -- --analyze",
    "reinstall": "node ./scripts/reinstall.js",
    "reinstall-force": "node ./scripts/reinstall.js -f",
    "test-smoke": "node scripts/test-smoke.js",
    "test-lighthouse": "node scripts/test-lighthouse.js 10",
    "test-lighthouse-max": "node scripts/test-lighthouse.js 100",
    "deploy": "npm run build -- --prod && call c:/code/eofol4-deploy.bat && npm run test-smoke && npm run test-lighthouse",
    "docker-build": "docker build -t eofol4-test ./docker-test",
    "docker-run": "docker run -p 3000:3000 eofol4-test",
    "docker-stop": "bash ./docker-test/docker-stop.sh",
    "docker-test": "npm run docker-stop && npm run docker-build && npm run docker-run"
  },
  "dependencies": {
    "bluebird": "^3.7.2",
    "chalk": "2.4.2",
    "cross-spawn": "^7.0.3",
    "csstype": "^3.1.3",
    "dotenv": "^16.4.5",
    "hex-to-css-filter": "^5.4.0",
    "html-minifier-terser": "^7.2.0",
    "html-to-json-parser": "^2.0.1",
    "rimraf": "^6.0.1",
    "sharp": "^0.33.5",
    "svgo": "^3.3.2",
    "ts-loader": "^9.5.1",
    "typescript": "^5.5.4",
    "uglify-js": "^3.19.0",
    "watchpack": "^2.4.2",
    "webpack": "^5.93.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.12",
    "eofol-live-server": "^1.2.4",
    "eslint": "^9.9.0",
    "globals": "^15.9.0",
    "jest": "^29.7.0",
    "prettier": "3.3.3",
    "ts-jest": "^29.2.4",
    "typescript-eslint": "^8.1.0",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "author": "wurducius@gmail.com",
  "bugs": "https://github.com/wurducius/eofol4/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wurducius/eofol4.git"
  },
  "license": "MIT"
}
